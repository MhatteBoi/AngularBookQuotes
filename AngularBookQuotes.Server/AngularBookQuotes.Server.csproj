    <Content Remove="wwwroot\angularbookquotes.client\browser\media\fa-v4compatibility.woff2" />
    <Content Remove="wwwroot\angularbookquotes.client\browser\polyfills.js" />
    <Content Remove="wwwroot\angularbookquotes.client\browser\polyfills.js.map" />
    <Content Remove="wwwroot\angularbookquotes.client\browser\scripts.js" />
    <Content Remove="wwwroot\angularbookquotes.client\browser\scripts.js.map" />
    <Content Remove="wwwroot\angularbookquotes.client\browser\styles.css" />
    <Content Remove="wwwroot\angularbookquotes.client\browser\styles.css.map" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Authentication.JwtBearer" Version="8.0.8" />
    <PackageReference Include="Microsoft.AspNetCore.SpaProxy">
      <Version>8.*-*</Version>
    </PackageReference>
    <PackageReference Include="Swashbuckle.AspNetCore" Version="6.4.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\angularbookquotes.client\angularbookquotes.client.esproj">
      <ReferenceOutputAssembly>false</ReferenceOutputAssembly>
    </ProjectReference>
  </ItemGroup>

  <ItemGroup>
