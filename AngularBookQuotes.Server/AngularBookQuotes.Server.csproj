<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <SpaRoot>..\angularbookquotes.client</SpaRoot>
    <SpaProxyLaunchCommand>npm start</SpaProxyLaunchCommand>
    <SpaProxyServerUrl>https://localhost:4200</SpaProxyServerUrl>
  </PropertyGroup>

  <ItemGroup>
    <Content Remove="wwwroot\angularbookquotes.client\browser\app.component.css.map" />
    <Content Remove="wwwroot\angularbookquotes.client\browser\favicon.ico" />
    <Content Remove="wwwroot\angularbookquotes.client\browser\index.html" />
    <Content Remove="wwwroot\angularbookquotes.client\browser\main.js" />
    <Content Remove="wwwroot\angularbookquotes.client\browser\main.js.map" />
    <Content Remove="wwwroot\angularbookquotes.client\browser\media\fa-brands-400.ttf" />
    <Content Remove="wwwroot\angularbookquotes.client\browser\media\fa-brands-400.woff2" />
    <Content Remove="wwwroot\angularbookquotes.client\browser\media\fa-regular-400.ttf" />
    <Content Remove="wwwroot\angularbookquotes.client\browser\media\fa-regular-400.woff2" />
    <Content Remove="wwwroot\angularbookquotes.client\browser\media\fa-solid-900.ttf" />
    <Content Remove="wwwroot\angularbookquotes.client\browser\media\fa-solid-900.woff2" />
