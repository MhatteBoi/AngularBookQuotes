    <None Include="wwwroot\angularbookquotes.client\browser\main.js.map" />
    <None Include="wwwroot\angularbookquotes.client\browser\media\fa-brands-400.ttf" />
    <None Include="wwwroot\angularbookquotes.client\browser\media\fa-brands-400.woff2" />
    <None Include="wwwroot\angularbookquotes.client\browser\media\fa-regular-400.ttf" />
    <None Include="wwwroot\angularbookquotes.client\browser\media\fa-regular-400.woff2" />
    <None Include="wwwroot\angularbookquotes.client\browser\media\fa-solid-900.ttf" />
    <None Include="wwwroot\angularbookquotes.client\browser\media\fa-solid-900.woff2" />
